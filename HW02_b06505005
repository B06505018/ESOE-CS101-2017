def bin2int(N):
 +    
 +    L = int(len(str(N)))  
 +    A = 0                 
 +    K = 0
 +    while L > K :         
 +        r = int(N%10)     
 +        A = A + (2**K)*r 
 +        N = N/10          
 +        K = K + 1         
 +    
 +    str(A)                
 +    return A
